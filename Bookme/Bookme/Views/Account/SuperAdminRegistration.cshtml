
@{
    ViewData["Title"] = "SuperAdminRegistration";
}
<div class="container">
    <div class="mt-5">
        <div class=" text-dark text-primary text-center p-5  mt-5">
            <h3 style="color:#4a0000;"><b>Enter your Details to Register</b></h3>
        </div>

            <div class="card shadow mt- w-50 mx-auto mb-5">
            
                <div class="card-body">
                    <form>
                        <div class="row">
                            <div class="col-6 mt-3">
                                <div class="form-group">
                                <label style="border-color: #4a0000" for="firstName">First Name:</label>
                                <input type="text" style="border-color: #4a0000" class="form-control" id="firsname" placeholder="Enter first name" name="">
                                </div>
                            </div>
                            <div class="col-6 mt-3">
                                <div class="form-group">
                                <label style="border-color: #4a0000" for="lastName">Last Name:</label>
                                <input type="text" style="border-color: #4a0000" class="form-control" id="lastname" placeholder="Enter last name" name="last name">
                                </div>
                            </div>
                        </div>
                       
                        <div class="row">
                            <div class="col-6 mt-3">
                            <div class="form-group">
                                <label style="border-color: #4a0000" for="email">Email:</label>
                                <input type="email" style="border-color: #4a0000"  class="form-control" id="email" placeholder="Enter email" name="email">
                            </div>
                            </div>
                            <div class="col-6 mt-3">
                                <div class="form-group">
                                <label style="border-color: #4a0000" for="price">User Name:</label>
                                <input type="text" style="border-color: #4a0000" class="form-control" id="userName" placeholder="Enter username" name="">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6 mt-3">
                                <div class="form-group">
                                <label style="border-color: #4a0000" for="pwd">password:</label>
                                <input type="password" style="border-color: #4a0000" class="form-control" id="pwd" placeholder="Enter password" name="pswd">
                                </div>
                            </div>
                            <div class="col-6 mt-3">
                                <div class="form-group">
                                <label style="border-color: #4a0000" for="confirm pwd">Confirm Password:</label>
                                <input type="password" style="border-color: #4a0000" class="form-control" id="confirmpwd" placeholder="Confirm password" name="pswd">
                                </div>
                            </div>
                        </div>
                    <div class="row">
                        <div class="col-12 mt-3">
                            <div class="form-group">
                                <label style="border-color: #4a0000" for="number">Phone Number:</label>
                                <input  style="border-color: #4a0000" class="form-control" id="phonenumber" placeholder="Enter phone number" name="pswd">
                            </div>
                        </div>
                    </div>
                   
                        <div class="text-center">
                        <button type="button" class="btn text-white mt-3" style="background-color:#4a0000;" onclick="superAdminRegistration()"><b>Submit</b></button>
                        </div>
                    </form>
                </div>
            </div>
    </div>
</div>